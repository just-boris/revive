{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
      "react"
  ],
  "globals": {
      "MOCK_REQUEST": true
  },
  "rules": {
    "quotes": [2, "single"],
    "one-var": [2, "never"],
    "react/jsx-no-undef": 2,
    "react/no-unknown-property": 2
  }
}
